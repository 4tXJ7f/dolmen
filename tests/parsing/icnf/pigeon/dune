; File generated for tests by a script

; Test for hole10.icnf
(rule
  (target  hole10.output)
  (deps    hole10.icnf)
  (package dolmen_bin)
  (action (chdir %{workspace_root} (with-outputs-to %{target} (run dolmen --type=false %{deps}))))
)
(rule
  (alias runtest)
  (action (diff hole10.output hole10.expected))
)

