; File generated for tests by a script

; Test for hanoi4.icnf
(rule
  (target  hanoi4.output)
  (deps    hanoi4.icnf)
  (package dolmen_bin)
  (action (chdir %{workspace_root} (with-outputs-to %{target} (run dolmen %{deps}))))
)
(rule
  (alias runtest)
  (action (diff hanoi4.output hanoi4.expected))
)

